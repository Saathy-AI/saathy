# .env.example
# Copy this file to .env and fill in the values.
# This file is for documentation purposes and should not contain any secrets.

# Application settings
ENVIRONMENT=development # development or production
DEBUG=true
LOG_LEVEL=INFO

# Qdrant settings
QDRANT_URL=http://localhost:6333

QDRANT_API_KEY=your_qdrant_api_key_here
QDRANT_COLLECTION_NAME=documents
QDRANT_VECTOR_SIZE=384

# OpenAI settings
OPENAI_API_KEY=your-openai-api-key # Used for local dev if not using docker secrets
OPENAI_MODEL=gpt-3.5-turbo

# Observability settings
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4317
OTEL_SERVICE_NAME=saathy-api
ENABLE_TRACING=true

# Grafana settings
GF_SECURITY_ADMIN_PASSWORD=your-grafana-admin-password
